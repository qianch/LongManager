<LongPages:BasePage x:Class="LongManager.Pages.Config.SystemConfig"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:LongPages="clr-namespace:LongManager.Pages"
      xmlns:local="clr-namespace:LongManager.Pages.Config"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SystemConfig" Loaded="BasePage_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Margin="24 0 0 0" VerticalAlignment="Center">
            <TextBlock Style="{StaticResource MaterialDesignDisplay1TextBlock}" TextWrapping="Wrap">系统设置</TextBlock>
        </StackPanel>
    </Grid>
</LongPages:BasePage>
